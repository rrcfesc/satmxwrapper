{
    "name": "rioxygen/satwrapper",
    "description": "PHPWrapper to navigate into the SAT MX",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo Ruiz",
            "email": "rrcfesc@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Rioxygen\\SatMx\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rioxygen\\SatMx\\": "tests"
        }
    },
    "require": {
        "php": ">=7.3",
        "ext-curl": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-filter": "*",
        "symfony/filesystem": "^5",
        "eclipxe/cfdiutils": "^2.12",
        "symfony/process": "^5",
        "ext-openssl": "^7.3"
    },
    "minimum-stability": "stable",
    "config": {
        "bin-dir": "bin"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "^3.5"
    }
}
